{
  "@context": {
    "@base": "http://purl.org/ccf/latest/ccf.owl#",
    "@vocab": "http://purl.org/ccf/latest/ccf.owl#",
    "ccf": "http://purl.org/ccf/latest/ccf.owl#",
    "rdf": "http://www.w3.org/2000/01/rdf-schema#",
    "obo": "http://purl.obolibrary.org/obo/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "dc": "http://purl.org/dc/elements/1.1/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "label": "rdf:label",
    "creator": "dc:creator",
    "creation_date": {
      "@id": "creation_date",
      "@type": "xsd:date"
    },
    "representation_of": {
      "@id": "ccf_representation_of",
      "@type": "@id"
    },
    "ccf_annotations": {
      "@id": "ccf_annotation",
      "@type": "@id",
      "@container": "@set"
    },
    "freetext_annotations": {
      "@id": "ccf_freetext_annotation",
      "@type": "xsd:string",
      "@container": "@set"
    },

    "object": {
      "@id": "has_object_reference"
    },
    "file": {
      "@id": "has_object_file"
    },
    "file_format": {
      "@id": "has_object_file_format"
    },
    "file_subpath": {
      "@id": "has_object_file_subpath"
    },

    "placement": {
      "@reverse": "has_placement_source"
    },
    "target": {
      "@id": "has_placement_target",
      "@type": "@id"
    },
    "placement_date": {
      "@id": "has_placement_date",
      "@type": "xsd:date"
    },

    "x_dimension": { 
      "@id": "has_x_dimension",
      "@type": "xsd:float"
    },
    "y_dimension": { 
      "@id": "has_y_dimension",
      "@type": "xsd:float"
    },
    "z_dimension": { 
      "@id": "has_z_dimension",
      "@type": "xsd:float"
    },
    "dimension_units": {
      "@id": "has_dimension_units"
    },

    "x_scaling": { 
      "@id": "has_x_scaling",
      "@type": "xsd:float"
    },
    "y_scaling": { 
      "@id": "has_y_scaling",
      "@type": "xsd:float"
    },
    "z_scaling": { 
      "@id": "has_z_scaling",
      "@type": "xsd:float"
    },

    "x_rotation": { 
      "@id": "has_x_rotation",
      "@type": "xsd:float"
    },
    "y_rotation": { 
      "@id": "has_y_rotation",
      "@type": "xsd:float"
    },
    "z_rotation": { 
      "@id": "has_z_rotation",
      "@type": "xsd:float"
    },
    "w_rotation": { 
      "@id": "has_theta_rotation",
      "@type": "xsd:float"
    },
    "rotation_order": {
      "@id": "has_rotation_order"
    },
    "rotation_units": {
      "@id": "has_rotation_units"
    },

    "x_translation": { 
      "@id": "has_x_translation",
      "@type": "xsd:float"
    },
    "y_translation": { 
      "@id": "has_y_translation",
      "@type": "xsd:float"
    },
    "z_translation": { 
      "@id": "has_z_translation",
      "@type": "xsd:float"
    }
  }
}